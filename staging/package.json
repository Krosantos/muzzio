{
  "name": "muzzio",
  "version": "1.0.0",
  "description": "A nerdy tool to help build EDH decks",
  "main": "./index.js",
  "repository": "https://github.com/Krosantos/muzzio.git",
  "author": "Krosantos <tymko.tecca@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npx webpack",
    "clean": "node electron/runClean",
    "compile-mac": "yarn build && node electron/prebuild && electron-packager ./staging \"Muzzio\" --asar --icon=./icon --packageManager=yarn --appCategoryType=public.app-category.utilities --out=compiled/ --platform=darwin --no-tmpdir --overwrite",
    "compile-windows": "yarn build && node electron/prebuild && electron-packager ./staging \"Muzzio\" --asar --icon=./icon --packageManager=yarn --out=compiled/ --platform=win32 --no-tmpdir --overwrite",
    "dev": "webpack-dev-server --mode development",
    "lint": "eslint .",
    "serve": "nodemon server/index.js --exec \"node -r dotenv/config -r @babel/register\"",
    "start": "concurrently --kill-others \"yarn dev\" \"yarn window\"",
    "window": "cross-env FROM_LOCAL=true electron ."
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^4.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^13.0.1",
    "eslint": "^5.16.0",
    "eslint-config-tecca": "^1.0.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}